{
  "name": "cs2-inspect-serializer",
  "version": "1.0.0",
  "description": "Lightweight serializer for CS2 inspect links",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/csfloat/cs2-inspect-serializer.git"
  },
  "author": "CSFloat",
  "homepage": "https://github.com/csfloat/cs2-inspect-serializer",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start:ts": "npx ts-node src/index",
    "build": "tsup",
    "test": "jest",
    "protoc": "npx protoc --ts_out src --proto_path protos protos/econ.proto"
  },
  "devDependencies": {
    "@protobuf-ts/plugin": "^2.11.1",
    "@types/jest": "^30.0.0",
    "buffer": "^6.0.3",
    "crc-32": "^1.2.2",
    "jest": "^30.1.3",
    "ts-jest": "^29.4.3",
    "ts-node": "^10.9.2",
    "tsup": "8.5",
    "typescript": "^5.9.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/tests/**/*.test.ts"
    ]
  }
}